        eType="#//Courses" eOpposite="#//Courses/offeredBy"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Attendance">
    <eOperations name="markAttendance"/>
    <eOperations name="viewAttendance"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalNoOfClasses" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalAbsents" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="totalLates" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Teacher">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Int"
        iD="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="position" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="teaches" upperBound="-1"
        eType="#//Courses" eOpposite="#//Courses/taughtBy"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Transcript"/>
  <eClassifiers xsi:type="ecore:EClass" name="Fee"/>
  <eClassifiers xsi:type="ecore:EClass" name="Loan"/>
  <eClassifiers xsi:type="ecore:EEnum" name="CampusLocation">
    <eLiterals name="Lahore"/>
    <eLiterals name="Islamabad" value="1"/>
    <eLiterals name="Faisalabad" value="2"/>
    <eLiterals name="Karachi" value="3"/>
    <eLiterals name="Peshawar" value="4"/>
  </eClassifiers>
</ecore:EPackage>
