    <eStructuralFeatures xsi:type="ecore:EReference" name="offeredBy" eType="#//Department"
        eOpposite="#//Department/offers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="attendance" upperBound="-1"
