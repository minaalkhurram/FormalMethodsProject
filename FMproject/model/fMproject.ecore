  <eClassifiers xsi:type="ecore:EClass" name="Student">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="oneViewCourses threeCourseType threeChooseElectiveCourses fourStudentType fiveWithdrawBeforeAnnouncedDate sevenDropCourseWithinTwoWeeks ninecalculateCgpa tenthRepeatFailedCourses elevenRepeatCoursesForLowGPA tweleveRepeatCourseCondition forteenViewWarningStatus fifteenIssueWarningAtSemesterEnd sixteenRegisterCourseWithWarning seventeenUpdateWarningCountAfterSemester eighteenResetWarningCount ninteenAdmissionCancellation twentyViewFeeDetails twentyfourEligibility twentyfiveLoan twentyEight thirtyfive thirtySix thirtyseven thirtyeight thirtyNine"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="oneViewCourses" value="&#xA;&#x9;&#x9;self.studies.offeredIn->exists(semester | &#xA;&#x9;&#x9;&#x9;semester.offers->exists(course | &#xA;&#x9;&#x9;&#x9;&#x9;course.offeredBy &lt;> null&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;)"/>
      <details key="threeCourseType" value=" let coreCourses = self.offered.coreCourses in&#xA;&#x9;&#x9;coreCourses->forAll(course | self.hasCompletedCourse->includes(course)) &#xA;&#x9;&#x9;implies self.offered.isCompleted = true"/>
      <details key="threeChooseElectiveCourses" value="(let electiveCourses = self.studies->select(course | course.courseType = CourseType::elective) in&#xA;&#x9;     electiveCourses->size() > 0)"/>
      <details key="fourStudentType" value="&#xA;         if self.oclIsTypeOf(BachelorStudent) then&#xA;        self.studies->size() &lt;= 5&#xA;         else&#xA;        self.studies->size() &lt;= 3&#xA;         endif"/>
      <details key="fiveWithdrawBeforeAnnouncedDate" value="&#xA;    (self.studiedBy ->forAll(reg |&#xA;        reg.withdrawalDate &lt;> null and ( reg.canWithdraw()=true)&#xA;    ))"/>
      <details key="sevenDropCourseWithinTwoWeeks" value="&#xA;         &#x9;self.studiedBy->forAll(reg | reg.dropDate->notEmpty() and reg.registerationDate->notEmpty()&#xA;         &#x9;and reg.canDrop()=true)"/>
      <details key="ninecalculateCgpa" value="&#xA;     let totalCredits : Integer = self.hasTranscript.courses->collect(course | course.creditHours)->sum()&#xA;     , &#xA;     totalGradePoints : _'ecore.xml.type'::Double[1] = self.hasTranscript.courses->collect(course | course.hasResult.hasGrade.getGradePoint() )->sum()&#xA;      in&#xA;            self.CGPA = totalGradePoints/totalCredits"/>
      <details key="tenthRepeatFailedCourses" value="self.hasCompletedCourse->forAll(course | course.hasResult->isEmpty() or course.hasResult.hasGrade.GradeType &lt;> 'F')"/>
      <details key="elevenRepeatCoursesForLowGPA" value="&#xA;        self.warningCount > 0 and&#xA;                self.CGPA &lt; self.offered.minimumCGPA and&#xA;        self.hasCompletedCourse->exists(course |&#xA;            let result : Result = course.hasResult in&#xA;            result.hasGrade.GradeType=_'Grade Type'::C or  result.hasGrade.GradeType=_'Grade Type'::_'C-' or&#xA;            result.hasGrade.GradeType=_'Grade Type'::D or result.hasGrade.GradeType=_'Grade Type'::_'D-'   or&#xA;            result.hasGrade.GradeType=_'Grade Type'::F&#xA;        )"/>
      <details key="tweleveRepeatCourseCondition" value="&#xA;        self.studies.hasResult->exists(result |&#xA;        result.studentID = self.rollNumber and &#xA;            result.hasGrade.GradeType=  _'Grade Type'::F   or result.hasGrade.GradeType=  _'Grade Type'::W or&#xA;            &#xA;             result.hasGrade.GradeType &lt;>  _'Grade Type'::_'A*' or   result.hasGrade.GradeType&lt;>  _'Grade Type'::A&#xA;            &#xA;        )"/>
      <details key="forteenViewWarningStatus" value="self.warningCount->notEmpty() implies self.viewWarning() &lt;> null"/>
      <details key="fifteenIssueWarningAtSemesterEnd" value="&#xA;            self.studiedBy->exists(sem | &#xA;             self.hasSemester.semesterType = SemesterType::Fall or&#xA;             self.hasSemester.semesterType = SemesterType::Spring or&#xA;             self.hasSemester.semesterType = SemesterType::Summer&#xA;             ) implies&#xA;               self.CGPA &lt; self.offered.minimumCGPA implies&#xA;               self.warningCount > self.Issued->size()@pre"/>
      <details key="sixteenRegisterCourseWithWarning" value="self.warningCount > 0 and self.getApprovalByHOD() implies self.registerCourses()"/>
      <details key="seventeenUpdateWarningCountAfterSemester" value="&#xA;        self.studiedBy->notEmpty() and self.studiedBy->exists(registeration |&#xA;        registeration.registerationStatus = RegisterationStatus::_'not registered' and&#xA;        self.CGPA &lt; self.offered.minimumCGPA and&#xA;        self.warningCount = 1&#xA;        )"/>
      <details key="eighteenResetWarningCount" value="&#xA;        (self.warningCount >= 3 and self.CGPA >= self.offered.minimumCGPA) implies&#xA;        self.warningCount = 0"/>
      <details key="ninteenAdmissionCancellation" value="&#xA;        let maxDuration : Integer = 10&#xA;        in&#xA;            self.calculateEnrollmentDuration() &lt;= maxDuration"/>
      <details key="twentyViewFeeDetails" value=" self.viewFeeDetails() &lt;> null"/>
      <details key="twentyfourEligibility" value="self.hasPaidAllDues or self.getApprovalByHOD() implies  self.canAppearinExams()"/>
      <details key="twentyfiveLoan" value=" self.studentType = _'Student Type'::indigent implies&#xA;        (&#xA;        self.applyForLoan() &lt;> null&#xA;        )"/>
      <details key="twentyEight" value=" self.degree = 'Bachelor' implies self.CGPA >= 2.0 implies self.hasLoan.loanStatus = LoanStatus::Active&#xA;        and&#xA;        self.degree = 'Master' implies self.CGPA >= 2.5 implies self.hasLoan.loanStatus = LoanStatus::Active&#xA;        and&#xA;        self.hasLoan.loanStatus = LoanStatus::Active implies (self.degree = 'Bachelor' and self.CGPA &lt; 2.0)&#xA;        or (self.degree = 'Master' and self.CGPA &lt; 2.5)"/>
      <details key="thirtyfive" value="self.studies->forAll(c | c.hasAttendance->notEmpty())"/>
      <details key="thirtySix" value="self.studies->forAll(course |  course.hasAttendance->notEmpty() and &#xA;&#x9;&#x9;&#x9;course.hasAttendance->forAll(attendance | attendance.attendancePercentage >= 0.8))"/>
      <details key="thirtyseven" value=" &#xA;&#x9;&#x9;&#x9;self.studies->forAll(course | Result.allInstances()->exists(result | result.studentID = self.rollNumber and result.courseID = course.courseCode))"/>
      <details key="thirtyeight" value="&#xA;&#x9;&#x9;&#x9; self.studies->forAll(course | &#xA;             Result.allInstances()->exists(result | &#xA;              result.studentID = self.rollNumber and &#xA;              result.courseID = course.courseCode and &#xA;              result.semesterID = self.hasSemester.semesterID&#xA;              ) implies self.applyForGradeChangeRequest())"/>
      <details key="thirtyNine" value=" self.studies->forAll(course |  self.applyForGradeChangeRequest() implies  course.offeredBy.hasHOD.approveRetake()=true)"/>
    </eAnnotations>
